@using Agility.Web.Plugins
@model MVC4SampleSite.Models.Module_SiteFeedbackForm

<h1>@Model.Title</h1>

<form id="form">
    <label>@Model.NameLabel</label>
    <input type="text" name="Name" />

    <label>@Model.EmailAddressLabel</label>
    <input type="text" name="EmailAddress" />

    <label>@Model.CommentLabel</label>
    <textarea name="Comment"></textarea>

    <input type="submit" value="Submit" />


    <div id="records"></div>


</form>


